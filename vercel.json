{
  "version": 2,
  "routes": [
    { "src": "/p/(?<id>[^/]+)/(?<slug>[^/]+)$", "dest": "/post?id=$id" },
    { "src": "/pr/(?<id>[^/]+)/(?<slug>[^/]+)$", "dest": "/prestacion?id=$id" },
    { "src": "/oo/(?<id>[^/]+)/(?<slug>[^/]+)$", "dest": "/oferta-on-line?id=$id" },
    { "src": "/c-o-o/(?<id>[^/]+)/(?<slug>[^/]+)$", "dest": "/category-ofertas-on-line?id=$id" },
    {
      "src": "/c-ca/(?<sid>[^/]+)/(?<slug>[^/]+)/(?<comunidad>[^/]+)/(?<caid>[^/]+)$",
      "dest": "/category-comunidad?sid=$sid&comunidad=$comunidad&caid=$caid"
    },
    {
      "src": "/c/(?<sid>[^/]+)/(?<slug>[^/]+)$",
      "dest": "/category?sid=$sid"
    },
    {
      "src": "/ca/(?<comunidad>[^/]+)/(?<caid>[^/]+)$",
      "dest": "/comunidad?comunidad=Catalu&caid=$caid"
    },
    {
      "src": "/m-o-g-m/(?<id>[^/]+)/(?<slug>[^/]+)$",
      "dest": "/ofertas-de-la-marca?id=$id"
    },
    {
      "src": "/m-o-g-m-ca/(?<id>[^/]+)/(?<slug>[^/]+)$",
      "dest": "/ofertas-de-la-marca-ca?id=$id"
    },
    {
      "src": "/mm/(?<id>[^/]+)/(?<slug>[^/]+)$",
      "dest": "/mapa-de-la-marca?id=$id"
    },
    {
      "src": "/ogm/(?<id>[^/]+)/(?<slug>[^/]+)$",
      "dest": "/oferta-gran-marca?id=$id"
    },
    {
      "src": "/ogmca/(?<id>[^/]+)/(?<slug>[^/]+)$",
      "dest": "/oferta-gran-marca-ca?id=$id"
    },
    {
      "src": "/l/(?<localidad>[^/]+)/(?<slug>[^/]+)$",
      "dest": "/localidad?localidad=$localidad"
    },
    {
      "src": "/manifest.json",
      "dest": "/static/manifest.json"
    },
    {
      "src": "/google614be494af9bad15.html",
      "dest": "/static/google614be494af9bad15.html"
    },
    {
      "src": "^/service-worker.js$",
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/sitemap.xml",
      "dest": "/static/sitemap.xml"
    },
    {
      "src": "/robots.txt",
      "dest": "/static/robots.txt"
    }
  ]
}